 RowBox[{
  RowBox[{
   RowBox[{"tpMillerRabin", "[", "n_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "a", ",", "resultado", ",", "b", ",", "d", ",", "s", ",", "t", ",", "c",
