Cell[BoxData[
 RowBox[{
  RowBox[{"esnatural", "[", "n_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"list", ",", "i"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"list", "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"n", ">", "0"}], " ", "&&", 
        RowBox[{"IntegerQ", "[", "n", "]"}]}], ",", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"i", "=", "0"}], ",", 
         RowBox[{"i", "<", "n"}], ",", 
         RowBox[{"i", "=", 
          RowBox[{"i", "+", "1"}]}], ",", 
         RowBox[{"AppendTo", "[", 
          RowBox[{"list", ",", "i"}], "]"}]}], "]"}], ",", 
       RowBox[{"Print", "[", 
        RowBox[{"n", " ", "\"\<no es un n\[UAcute]mero natural\>\""}], 
        "]"}]}], "]"}], ";", "\[IndentingNewLine]", "list"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
