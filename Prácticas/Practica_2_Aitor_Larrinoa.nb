Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"esgeneradora", "[", 
     RowBox[{"p_", ",", "A_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"k", ",", "n", ",", "i", ",", "list"}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"k", "=", 
        RowBox[{"Length", "[", "A", "]"}]}], ";", 
       RowBox[{"n", "=", 
        RowBox[{"Length", "[", 
         RowBox[{"A", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"list", "=", 
        RowBox[{"{", 
         RowBox[{"-", "1"}], "}"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"PrimeQ", "[", "p", "]"}], " ", ",", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"k", "\[LessEqual]", "n"}], ",", "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"MatrixRank", "[", 
               RowBox[{"A", ",", 
                RowBox[{"Modulus", "\[Rule]", "p"}]}], "]"}], "\[Equal]", 
              "k"}], ",", "\[IndentingNewLine]", 
             RowBox[{"list", "=", 
              RowBox[{"Mod", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"Tuples", "[", 
                  RowBox[{
                   RowBox[{"esnatural", "[", "p", "]"}], ",", "k"}], "]"}], 
                 ".", "A"}], ",", "p"}], "]"}]}], ",", 
             RowBox[{
             "Print", "[", 
              "\"\<La matriz introducida no puede generar un c\[OAcute]digo \
lineal\>\"", "]"}]}], "]"}], ",", 
           RowBox[{
           "Print", "[", "\"\<Las dimensiones de M no son adecuadas\>\"", 
            "]"}]}], "]"}], ",", 
         RowBox[{"Print", "[", 
          RowBox[{"p", " ", "\"\<no es primo\>\""}], "]"}]}], "]"}], ";", 
       "\[IndentingNewLine]", "list"}]}], "\[IndentingNewLine]", "]"}]}], 
   ";"}], "\[IndentingNewLine]"}]], "Input",
